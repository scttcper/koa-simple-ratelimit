{
  "name": "koa-simple-ratelimit",
  "version": "2.4.1",
  "description": "Simple Rate limiter middleware for koa v2",
  "repository": "scttcper/koa-simple-ratelimit",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "koa",
    "middleware",
    "ratelimiter",
    "ratelimit",
    "rate"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "ms": "^2.1.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/jest": "25.1.1",
    "@types/koa": "2.11.0",
    "@types/node": "13.7.0",
    "@types/redis": "2.8.14",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.22.0",
    "eslint-config-xo-typescript": "0.24.1",
    "eslint-plugin-import": "2.20.0",
    "ioredis": "4.14.1",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "koa": "2.11.0",
    "redis": "2.8.0",
    "semantic-release": "17.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.1.0",
    "typescript": "3.7.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8"
  }
}
